{
    "manifest_version": 2,
    "name" : "QuoteMe",
    "version" :"1.0",
    "description" :"This Extension provides you random Quotes",
    "icons":{
        "128" :"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
    },

    "permissions" :[
        "storage",
        "notifications",
        "contextMenus",
        "*://localhost:*/*"
    ]
}